{
    "intel-analytics-config": {
      "destination": "/etc/intelanalytics/rest-server",
      "source": "etc/intelanalytics/rest-server",
      "priority": 10,
      "isDirectory": true
    },
    "intel-analytics-logrotate.d": {
      "destination": "/etc/logrotate.d/intelanalytics-rest-server",
      "source": "etc/logrotate.d/intelanalytics-rest-server",
      "priority": 10,
      "isDirectory": false
    },
    "intel-analytics-graphbuilder-lib": {
      "destination": "/usr/lib/intelanalytics/graphbuilder/lib/ispark-deps.jar",
      "source": "usr/lib/intelanalytics/graphbuilder/lib/ispark-deps.jar",
      "priority": 10,
      "isDirectory": false
    }
}