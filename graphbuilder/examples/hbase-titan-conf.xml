<!-- This is the configuration file used when bulk loading the Titan graph 
	database. The configuration parameters are documented at https://github.com/thinkaurelius/titan/wiki/Using-HBase 
	Currently GB 2.0 (alpha) supports only HBase as the storage backend. -->

<configuration>
	<property>
		<name>graphbuilder.titan.storage_hostname</name>
		<value>localhost</value>
		<description>
			storage.hostname parameter for Titan.
		</description>
	</property>

	<property>
		<name>graphbuilder.titan.storage_tablename</name>
		<value>test_graph</value>
		<description>
			storage.tablename parameter for Titan.
		</description>
	</property>

	<property>
		<name>graphbuilder.titan.storage_backend</name>
		<value>hbase</value>
		<description>
			storage.backend parameter for Titan. Currently GB 2.0
			(alpha) supports only HBase as the storage backend.
		</description>
	</property>

	<property>
		<name>graphbuilder.titan.storage_port</name>
		<value>2181</value>
		<description>
			storage.port parameter for Titan.
		</description>
	</property>

	<property>
		<name>graphbuilder.titan.storage_connection_timeout</name>
		<value>10001</value>
		<description>
			storage.connection.timeout parameter for Titan.
		</description>
	</property>
</configuration>
