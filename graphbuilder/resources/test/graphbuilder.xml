<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!-- sample config file for graphbuilder.
     These options can also be specified in the hadoop options config file if you would like to avoid specifying it on
     the command line.
     Individual options can be specified on the
     command line with -D, -DmyProperty=myPropertyValue or -D myProperty=myPropertyValue
 -->

<configuration>
  <property>
    <name>graphbuilder.titan.storage_hostname</name>
    <value>localhost</value>
  </property>
<property>
	<name>graphbuilder.titan.storage_backend</name>
	<value>hbase</value>
</property>

 <property>
	<name>graphbuilder.titan.storage_tablename</name>
	<value>rodoradTitan</value>
</property>

<property>
        <name>graphbuilder.titan.storage_port</name>
        <value>2181</value>
</property>

<property>
        <name>graphbuilder.titan.storage_connection_timeout</name>
        <value>10001</value>
</property>

</configuration>

